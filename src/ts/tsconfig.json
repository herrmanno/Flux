{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "../js",
        "noEmitOnError": false,
        "isolatedModules": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "../typings.d.ts",
        "!./node_modules/**/*"
    ],
    "files": [
        "./callbackholder.ts",
        "./config.ts",
        "./dispatcher.ts",
        "./flux.ts",
        "./router.ts",
        "./state.ts",
        "./stateprovider.ts",
        "./store.ts",
        "./storeprovider.ts",
        "./storeregistry.ts",
        "../typings.d.ts"
    ]
}
