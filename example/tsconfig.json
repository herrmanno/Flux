{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*",
        "!./bower_components/**/*"
    ],
    "files": [
        "./components/App/app.ts",
        "./components/Loginform/loginform.ts",
        "./components/Navbar/navbar.ts",
        "./components/Test/test.ts",
        "./components/View/view.ts",
        "./states.ts",
        "./stores/LoginStore/LoginStore.ts",
        "./stores/Myrouter/MyRouter.ts",
        "./stores/UserStore/UserStore.ts"
    ]
}
